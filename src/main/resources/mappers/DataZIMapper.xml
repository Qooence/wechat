    <!--oracle  oracle 没有limit -->
    <select id="findByIndex_" resultMap="BaseResultMap">
        SELECT
            *
        FROM
            data_zi
        <where>
            rownum &lt;= 63
            <if test="index_ != null and index_ != ''">
                AND index_ = #{index_}
            </if>
        </where>
        ORDER BY sort ASC
    </select>

    <delete id="clear">
        delete from data_zi
    </delete>

</mapper>